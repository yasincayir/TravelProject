@model TravelProject.Models.Siniflar.Blog
@{
    ViewBag.Title = "YeniBlog";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Yeni Blog Ekleme</h2>
<hr />
<br />
<form class="form-group" method="post">
    @Html.LabelFor(k => k.Baslik)
    @Html.TextBoxFor(k => k.Baslik, new { @class = "form-control" })
    <br />
    @Html.LabelFor(k => k.Tarih)
    @Html.TextBoxFor(k => k.Tarih, new { @class = "form-control" })
    <br />
    @Html.LabelFor(k => k.BlogImage)
    @Html.TextBoxFor(k => k.BlogImage, new { @class = "form-control" })
    <br />
    @Html.LabelFor(k => k.Aciklama)
    @Html.TextAreaFor(k => k.Aciklama, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Blog Ekle</button>
</form>

